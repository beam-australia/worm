{
    "name": "beam-australia/worm",
    "type": "library",
    "description": "Wordpress ORM",
    "homepage": "https://github.com/beam-australia/worm",
    "keywords": [
        "wordpresss",
        "ORM",
        "database",
        "eloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "BeamAustralia.com.au",
            "homepage": "http://beamaustralia.com.au"
        },
        {
            "name": "Andrew McLagan",
            "email": "andrew@beamaustralia.com.au"
        }
    ],
    "require": {
        "fzaninotto/faker": "^1.8",
        "illuminate/support": "^5.8",
        "myclabs/php-enum": "^1.7",
        "nesbot/carbon": "^2.27",
        "tightenco/collect": "^5.8"
    },
    "require-dev": {
        "wp-phpunit/wp-phpunit": "^5.2",
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Beam\\Worm\\": "src/"
        },
        "files": [
            "src/functions.php",
            "config.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Tests\\Fixtures\\": "tests/__fixtures__/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
